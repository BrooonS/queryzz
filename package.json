{
  "name": "queryzz",
  "version": "1.0.0",
  "description": "Library for working with query",
  "main": "dist/queryzz.js",
  "types": "dist/queryzz.d.ts",
  "module": "dist/queryzz.js",
  "prepublish": "npm run build && npm run test",
  "scripts": {
    "dev": "webpack --env development --watch",
    "build": "webpack --env production",
    "test": "jest",
    "docs": "jsdoc2md --template ./README.hbs --files ./src/lib/*.ts --configure ./jsdoc2md.json > ./README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrooonS/queryzz.git"
  },
  "keywords": [
    "queryzz",
    "query",
    "set",
    "get",
    "format",
    "getQuery",
    "formatQuery",
    "setQuery"
  ],
  "author": "Valery Strelets",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BrooonS/queryzz/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm run test"
    }
  },
  "homepage": "https://github.com/BrooonS/queryzz#readme",
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-typescript": "7.12.7",
    "@types/jest": "26.0.20",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "@typescript-eslint/parser": "4.14.1",
    "babel-loader": "8.2.2",
    "eslint": "7.19.0",
    "eslint-config-airbnb-typescript": "12.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-webpack-plugin": "2.4.3",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "jsdoc-babel": "0.5.0",
    "jsdoc-to-markdown": "6.0.1",
    "react": "17.0.1",
    "ts-loader": "8.0.14",
    "typescript": "4.1.3",
    "webpack": "5.19.0",
    "webpack-cli": "4.4.0"
  }
}
